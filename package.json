{
  "name": "react-native-DDD-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:production:apk": "cd android && ./gradlew assembleRelease && ./gradlew installRelease && cd ..",
    "android:clean": "cd android/ && ./gradlew clean",
    "android:reverse": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios --scheme \"pudos\" --configuration Debug",
    "ios:staging": "react-native run-ios --scheme \"pudos-staging\" --configuration Release",
    "ios:production": "react-native run-ios --scheme \"pudos-production\" --configuration Release",
    "build:android": "cd android && ./gradlew app:assembleRelease && cd ..",
    "build:ios": "xcodebuild -workspace ios/pudos.xcworkspace -scheme pudos -configuration Release -derivedDataPath ios/build -destination 'name=iPhone 11'",
    "lint": "eslint index.js app storybook test --ext .js,.ts,.tsx",
    "lint:no-cycles": "eslint index.js app storybook test --ext .js,.ts,.tsx --rule 'import/no-cycle: 2'",
    "postinstall": "node ./bin/postInstall",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "tsc": "tsc --noEmit -p . --pretty",
    "fullClean": "rm -rf $HOME/.gradle/caches/ && yarn ac && cd .. rm -rf node_modules && yarn install",
    "install:pods": "cd ios && pod-install --quiet",
    "install:deps": "yarn install && yarn install:pods",
    "parseSchema": "npx openapi-typescript ./app/infrastructure/api/open-api.yml --output ./app/infrastructure/api/schema.ts",
    "sourceMap:ios": "react-native bundle --platform ios --dev false --bundle-output ./ios/main.jsbundle  --sourcemap-output ./sourcemap.js --entry-file index.js",
    "sourceMap:android": "react-native bundle --platform android --dev false --bundle-output ./android/main.jsbundle  --sourcemap-output ./sourcemap.js --entry-file index.js",
    "followStackTrace": "node followStackTrace.js",
    "gql:codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
